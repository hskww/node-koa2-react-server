{
  "name": "node-koa-react-server",
  "version": "1.0.0",
  "description": "server side react render for koa",
  "main": "src/server/app.js",
  "scripts": {
    "test": "make test",
    "start": "NODE_ENV=dev supervisor  src/server/app.js",
    "build": "webpack --config config/webpack.config.product.js",
    "dev": "webpack-dev-server -d --config config/webpack.config.dev.js",
    "watch": "node  src/server/koa-webpackdev.js"
  },
  "keywords": [
    "koa-react"
  ],
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.4.3",
    "babel-runtime": "^6.9.2",
    "copy-to": "~2.0.1",
    "escape-html": "~1.0.2",
    "html-webpack-plugin": "^2.22.0",
    "js-beautify": "^1.5.10",
    "jsx-loader": "~0.13.2",
    "koa": "^2.0.0",
    "koa-logger": "^1.3.0",
    "koa-route": "^2.4.2",
    "koa-static-cache": "~3.1.1",
    "koa-webpack-dev-server": "^0.3.0",
    "mocha": "~2.2.5",
    "should": "~6.0.3",
    "supertest": "~1.0.1",
    "unescape-html": "~1.0.0",
    "webpack": "~1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  }
}
